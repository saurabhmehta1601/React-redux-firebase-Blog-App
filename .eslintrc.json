{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console":"warn",
        "no-unused-vars":"warn",
        "jsx-a11y/anchor-is-valid":"off",
        "react/react-in-jsx-scope":"off",
        "react/prop-types": "off"
    }
}
